{
    "version": 2,
    "installCommand": "npm install",
    
    "config":[
        {
            "commands":[
                "npm install",
                "rm -rf node_modules",
                "npm install"
            ]
        }
    ],
    "builds": [
        {
            "src": "server.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "server.js"
        }
    ]
  }